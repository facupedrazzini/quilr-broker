{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "no-tabs": "off",
    "indent": [
      "error",
      "tab"
    ],
    "react/jsx-indent": [
      "error",
      "tab"
    ]
  }
}